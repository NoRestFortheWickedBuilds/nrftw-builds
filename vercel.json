{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "melee-tank.html",
      "use": "@vercel/static"
    },
    {
      "src": "bow.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles/**",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/melee-tank", "dest": "/melee-tank.html" },
    { "src": "/bow", "dest": "/bow.html" },
    { "src": "/styles/(.*)", "dest": "/styles/$1" }
  ]
}

